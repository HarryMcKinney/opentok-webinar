<%- include("includes/head-with-opentok") %>

<h1 class="page-title">Webinar: <%= webinar.name %></h1>

<div id="webinar" class="webinar-viewer" data-opentok="<%= JSON.stringify(opentok) %>">
  <div class="webinar-viewer-viewports no-video">
